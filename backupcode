ALTER TABLE `quote_terms` ADD `interest_type` VARCHAR(10) NULL DEFAULT NULL AFTER `interest_rate`;
ALTER TABLE `quote_terms` ADD `term_other_data` JSON NULL DEFAULT NULL AFTER `compensation`, ADD `agent_compensation_data` JSON NULL DEFAULT NULL AFTER `term_other_data`;
ALTER TABLE `entities` ADD `username` VARCHAR(100) NULL AFTER `type`;

ALTER TABLE `quote_policies` CHANGE `term` `policy_term` VARCHAR(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NULL DEFAULT NULL;

ALTER TABLE `quotes` ADD `quoteoriginationstate` VARCHAR(50) NULL AFTER `quote_notes`;

ALTER TABLE `company_fax_settings` ADD `use_subject` VARCHAR(50) NULL DEFAULT NULL AFTER `outgoing_fax_numbers`;
ALTER TABLE `quote_terms` ADD `setupfee_downpayment` DECIMAL(10,4) NULL DEFAULT NULL AFTER `setup_fee`;


ALTER TABLE `company_fax_settings` CHANGE `use_subject` `use_subject` VARCHAR(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NULL DEFAULT NULL, CHANGE `use_security_code` `use_security_code` VARCHAR(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NULL DEFAULT NULL, CHANGE `security_code` `security_code` VARCHAR(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NULL DEFAULT NULL;